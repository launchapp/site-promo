                    <h1>{{ contentVersion.title|raw }}</h1>
                    <div class="borderDashed">
                        <p class="fontPrimary">{{ contentVersion.description|raw }}</p>
                    </div>
                </div>
                <div id="tier1Right" class="pull-left">
                    <div id="tier1video" class="responsiveObject">
                        <iframe id="launchVideo" width="700" height="394" src="https://www.youtube.com/embed/IlANPQfnc_M?rel=0&showinfo=0&modestbranding=1&enablejsapi=1" frameborder="0" allowfullscreen></iframe>
                    </div>
                    <div class="borderDashed clearfix">
                        <div id="tier1action" class="pull-right">
                            <span class="fontPrimary bodyLarge optionalText">Help us build it!</span>
                            {#<a class="btn" href="">Kickstarter Campaign</a>#}
                            <div class="btn positionAnchor" data-plugin="AddClass Dim" data-class="active" data-target="#kickstarterSignup" data-event="click" >
                                <div class="btnText" data-trackCategory="CTA" data-trackLabel="kickstarter" data-trackValue="top">Kickstarter Campaign</div>
                                <div class="btnGradient"></div>
                            </div>
                        </div>
                    </div>
                </div>
        </div>
    </div>

    {# TIER 2 BROKEN STATUS QUO #}
